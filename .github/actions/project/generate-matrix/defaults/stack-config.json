{
    "nest": {
        "services": {
            "mssql": {
                "env": {
                    "ACCEPT_EULA": "Y",
                    "MSSQL_DB": "test_db",
                    "MSSQL_SA_PASSWORD": "Password123!"
                },
                "image": "PLACEHOLDER_MSSQL_IMAGE",
                "options": "--health-interval 10s --health-timeout 5s --health-retries 5 --health-cmd \"bash -c 'echo > /dev/tcp/localhost/1433'\"",
                "ports": [
                    "1433:1433"
                ]
            },
            "mysql": {
                "env": {
                    "MYSQL_DATABASE": "testdb",
                    "MYSQL_PASSWORD": "password",
                    "MYSQL_ROOT_PASSWORD": "rootpassword",
                    "MYSQL_USER": "mysql"
                },
                "image": "PLACEHOLDER_MYSQL_IMAGE",
                "options": "--health-interval 10s --health-timeout 5s --health-retries 5 --health-cmd \"mysqladmin ping\"",
                "ports": [
                    "3306:3306"
                ]
            },
            "nats": {
                "image": "PLACEHOLDER_NATS_IMAGE",
                "ports": [
                    "4222:4222"
                ]
            },
            "postgres": {
                "env": {
                    "POSTGRES_DB": "test_db",
                    "POSTGRES_HOST_AUTH_METHOD": "trust",
                    "POSTGRES_PASSWORD": "password",
                    "POSTGRES_USER": "postgres"
                },
                "image": "PLACEHOLDER_POSTGRES_IMAGE",
                "options": "--health-interval 10s --health-timeout 5s --health-retries 5 --health-cmd pg_isready",
                "ports": [
                    "5432:5432"
                ]
            },
            "redis": {
                "image": "PLACEHOLDER_REDIS_IMAGE",
                "ports": [
                    "6379:6379"
                ]
            },
            "typesense": {
                "env": {
                    "TYPESENSE_API_KEY": "api_key",
                    "TYPESENSE_DATA_DIR": "/home"
                },
                "image": "PLACEHOLDER_TYPESENSE_IMAGE",
                "options": "--health-interval 10s --health-timeout 10s --health-retries 5 --health-cmd=\"bash -c 'exec 3<>/dev/tcp/localhost/8108 && printf \\\"GET /health HTTP/1.1\\r\\nConnection: close\\r\\n\\r\\n\\\" >&3 && head -n1 <&3 | grep 200 && exec 3>&-'\"",
                "ports": [
                    "8108:8108"
                ]
            }
        }
    },
    "nuxt": {
        "services": {
        }
    },
    "vue": {
        "container": {
            "image": "PLACEHOLDER_VUE_CONTAINER_IMAGE",
            "options": "--user 1001"
        },
        "services": {
        }
    }
}
